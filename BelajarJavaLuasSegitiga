function TentukanLuasDanKelilingSegitigaSamaSisi(a, t) {
  const alas = a;
  const tinggi = t;
  // TODO: buat logic dibawah sini dimana alasnya 1 cm dan tingginya 2 cm
	

	const luas = (alas * tinggi) / 2;
  const keliling = 3 * alas;
  console.log("Alas Segitiga:", alas, "cm");
  console.log("Tingga Segitiga:", tinggi, "cm");
  console.log("Luas Segitiga:", luas, "cm^2");
  console.log("Keliling Segitiga:", keliling, "cm");
  // TODO: gunakan console.log untuk mencetak hasil luas dan keliling
  
  // Akhir pembuatan code
  //hello
  //thanks
